{
  "Routes": [
    {
      "DownstreamPathTemplate": "/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "odoo-api-middleware",
          "Port": 55005
        }
      ],
      "UpstreamPathTemplate": "/odooauth/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "Authentication"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Authentication",
      "Config": [
        {
          "Name": "Authentication & Authorization",
          "Version": "v1",
          "Url": "http://odoo-api-middleware:55005/swagger/v1/swagger.json"
        }
      ]
    }
